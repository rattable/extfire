{
  "manifest_version": 3,
  "name": "ExoCore / Roichi",
  "version": "1.0.0",
  "description": "Tableau de bord cyberpunk pour la gestion et l'audit des extensions.",
  "action": {
    "default_title": "ExoCore / Roichi",
    "default_popup": "index.html"
  },
  "permissions": [
    "management",
    "tabs",
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": ["<all_urls>"]
}
